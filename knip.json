{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.tsx",
    "scripts/**/*.js"
  ],
  "project": [
    "src/**/*.{ts,tsx}",
    "scripts/**/*.js"
  ],
  "ignore": [
    "src/vite-env.d.ts",
    "**/*.d.ts"
  ],
  "ignoreDependencies": [],
  "playwright": {
    "config": "playwright.config.ts",
    "entry": ["tests/**/*.ts"]
  },
  "vite": {
    "config": "vite.config.ts"
  },
  "eslint": {
    "config": "eslint.config.js"
  },
  "tailwind": {
    "config": "tailwind.config.ts"
  }
}
